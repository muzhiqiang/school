[32m++				contentType:"application/x-www-form-urlencoded; charset=UTF-8",[m
[32m++				data : {"Account":account, "controller":"researchGroup", "method":"showTeacherGroup"},[m
[32m++				datatype : "json",[m
[32m++				async: true,[m
[32m++				success:function(data) {[m
[32m++					if(data.success != true) {[m
[32m++						alert(data.success);[m
[32m++					}[m
[32m++					else {[m
[32m++						var tmp = "<option>è¯·é€‰æ‹©</option>";[m
[32m++						for(var p in data.data) {[m
[32m++[m
[32m++							tmp = tmp+'<option value="'+data.data[p].Res_group_id+'" >'+ data.data[p].Res_group_name+'</option>';[m
[32m++						}[m
[32m++						document.getElementById('searchGroup').innerHTML = tmp;[m
[32m++					}[m
[32m++[m
[32m++				},[m
[32m++				error: function(XMLHttpRequest, textStatus, errorThrown) {[m
